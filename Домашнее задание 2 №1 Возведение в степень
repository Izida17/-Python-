numbers = input("Введите числа через пробел: ").split()
step = int(input("Введите степень: "))

result = []
for i in numbers:
    if i.replace("-", "").replace(".", "", 1).isdigit():
        num = float(i)
        result.append(num ** step)
    else:
        result.append(i * step)

print("Вывод:", ' '.join(map(str, result)))
