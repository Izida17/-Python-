num_str = input("Введите число: ")
if num_str.isdigit():
  num = int(num_str)
  if num > 0:
    if num % 2 == 0:
      print(f"Число {num} является четным")
    else:
      print(f"Число {num} не является четным")
  else:
    print("Ошибка: введено не положительное число")
else:
  print("Ошибка: введено не число")
